{
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
        ],
    "env": {
        "node": true
    },
    "ignorePatterns": [
        "node_modules/",
        "test/**/*"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "import/no-cycle": "off",
        "no-shadow": "warn"
    }
}
